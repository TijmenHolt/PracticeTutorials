<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="estate2.list">
      <field name="name">estate2 list</field>
      <field name="model">estate2.estate2</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="estate2.action_window">
      <field name="name">estate2 window</field>
      <field name="res_model">estate2.estate2</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="estate2.action_server">
      <field name="name">estate2 server</field>
      <field name="model_id" ref="model_estate2_estate2"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="estate2" id="estate2.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="estate2.menu_1" parent="estate2.menu_root"/>
    <menuitem name="Menu 2" id="estate2.menu_2" parent="estate2.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="estate2.menu_1_list" parent="estate2.menu_1"
              action="estate2.action_window"/>
    <menuitem name="Server to list" id="estate2" parent="estate2.menu_2"
              action="estate2.action_server"/>

  </data>
</odoo>